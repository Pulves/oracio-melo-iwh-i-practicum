doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{title}
        link(rel="stylesheet", href="/css/style.css")
    body 
        h1 #{title}
        a(href="http://localhost/update-cobj") Add/update this table
        
        table 
            tr 
                th Name
                th Quantity
                th Amount 
                th Total Amount

            each customObject in customObjects
                tr
                    td #{customObject.name }
                    td #{customObject.quantity}
                    td #{customObject.amount}
                    td #{customObject.total_amount}

